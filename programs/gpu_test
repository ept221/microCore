		nop
		ldi r0, 0x00	// setup gpu pointer
		ldi r1, 0x20

poll:	in r2, 2		// read pin register
		ani r2, 2		// mask the button
		sll r2
		adi r2, 48
		str r2, p0
		jmp poll



